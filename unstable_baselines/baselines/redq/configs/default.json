{
  "env_name": "",
  "buffer":{
    "max_buffer_size": 1000000
  },
  "agent":{
    "gamma": 0.99,
    "reward_scale": 5.0,
    "update_target_network_interval": 1,
    "target_smoothing_tau": 0.005,
    "num_q_networks": 10,
    "num_q_samples": 2,
    "num_updates_per_iteration":20,
    "alpha": 0.2,
    "q_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "learning_rate":0.001,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "deterministic": false,
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity",
      "reparameterize": true
    },
    "entropy":{
      "automatic_tuning": true,
      "learning_rate": 0.001,
      "optimizer_class": "Adam"
    }
  },
  "trainer":{
    "max_env_steps": 500000,
    "batch_size": 100,
    "max_trajectory_length":1000,
    "eval_interval": 1000,
    "num_eval_trajectories": 10,
    "save_video_demo_interval": 1000,
    "start_timestep": 5000,
    "snapshot_interval": 5000,
    "log_interval": 1000
  }
}
