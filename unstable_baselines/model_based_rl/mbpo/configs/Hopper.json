{
  "env_name": "Hopper-v2",
  "buffer":{
    "max_buffer_size": 500000,
    "per": false
  },
  "agent":{
    "gamma": 0.99,
    "update_target_network_interval": 10,
    "target_smoothing_tau": 0.005,
    "alpha": 0.2,
    "q_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "deterministic": false,
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity",
      "re_parameterize": true
    },
    "entropy":{
      "automatic_tuning": true,
      "learning_rate": 0.0003,
      "optimizer_class": "Adam"
    },
    "transition_model":{
      "hidden_dims": [256, 256],
      "optimizer_class": "Adam",
      "learning_rate":0.0003,
      "num_models": 8,
      "num_elite": 5,
      "holdout_ratio": 0.1,
      "inc_var_loss": true,
      "env_name": "Hopper-v2"
    }
  },
  "trainer":{
    "max_epoch": 20000,
    "num_updates_per_iteration":50,
    "num_steps_per_iteration": 50,
    "update_n_interval": 50,
    "batch_size": 100,
    "max_trajectory_length":1000,
    "test_interval": 5,
    "num_test_trajectories": 5,
    "save_model_interval": 2000,
    "start_timestep": 1000,
    "save_video_demo_interval": 1000,
    "log_interval": 1, 
    "num_rollout_steps": 2,
    "num_model_steps": 1,
    "model_env_ratio": 0.8,
    "num_model_rollouts": 100,
    "num_updates_per_epoch": 20
  },
  "env":{
    "reward_scale": 10.0
  }
  
}
